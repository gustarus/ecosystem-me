.return
  display block
  width 100%
  height 50px
  position absolute
  left 0
  top 0
  background rgba(17, 17, 17, .1)
  transition background-color .2s
  box-shadow none
  &:hover
    background rgba(17, 17, 17, .3)
    box-shadow none
  &__house
    display block
    width 24px
    height 24px
    margin-top -12px
    margin-left -12px
    position relative
    left 50%
    top 50%
    fill #fff

